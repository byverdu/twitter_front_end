- var nav_icon = [{class: 'icon-home active', text: 'Home'}, {class: 'icon-bell', text: 'Notifications'}, {class: 'icon-slack', text: 'Discover'}, {class: 'icon-user', text: 'Me'}];

header.twitterHeader
  .twitterHeader__wrapper
  
    nav.twitterHeader__navIcons
      ul.twitterHeader__navIcons--list
        each val in nav_icon
          li(class="#{val.class} twitterHeader__navIcons--item")
            a(href="/"): span.text= val.text
        li.icon-twitter.twitterHeader__navIcons--item
    
    
    nav.twitterHeader__navSettings
      ul.twitterHeader__navSettings--list
        li.twitterHeader__navSettings--item
          input(type="text", placeholder=" Search Twitter")
          span.icon-search
        li.icon-envelope.twitterHeader__navSettings--item
        li.icon-cog.twitterHeader__navSettings--item
        li.icon-quill.twitterHeader__navSettings--item: a(href="#")
