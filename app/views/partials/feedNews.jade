.twitterFeeds__wrapper

  each tweet in initialTweetStore
    .twitterFeeds__wrapper--tweet.twitterFeeds__tweet
      if( tweet.retweet === true )
        .twitterFeeds__tweet--retweet
          span.icon-retweet
          span.twitterFeeds__tweet--retweetUser #{ tweet.whoRetweeted } retweeted
      include tweetUserProfile
          
      img( src=tweet.profile_image_url, alt=`image for ${ tweet.screen_name }` )
      
      .twitterFeeds__tweet--content
        h3 #{tweet.name} #[span= `@${tweet.screen_name}`]
        p #{tweet.text}
        if ( tweet.tweetImage !== 'no image')
          img(src="#{ tweet.tweetImage }", alt="image tweet")
      
      .twitterFeeds__tweet--social.twitterFeeds__social
        ul.twitterFeeds__social--list
          li.twitterFeeds__social--item.icon-reply
          li.twitterFeeds__social--item.icon-retweet
          li.twitterFeeds__social--item.icon-star
          li.twitterFeeds__social--item ...
    
